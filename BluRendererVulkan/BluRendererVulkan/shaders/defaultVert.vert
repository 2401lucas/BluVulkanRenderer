#version 450#extension GL_EXT_nonuniform_qualifier : enablelayout(location = 0) in vec3 inPosition;layout(location = 1) in vec3 inColour;layout(location = 2) in vec3 inNormal;layout(location = 3) in vec2 inTexCoord;layout(location = 4) in mat4 inMVP;layout(location = 8) in int inTexID;layout(location = 0) out vec3 fragColour;layout(location = 1) out vec3 fragNormal;layout(location = 2) out vec3 fragPosition;layout(location = 3) out vec3 fragTexCoord;void main() {	gl_Position = inMVP * vec4(inPosition, 1.0);	fragNormal = mat3(transpose(inverse(inMVP))) * inNormal;	fragColour = inColour;	fragPosition = vec3(inMVP * vec4(inPosition, 1.0));	fragTexCoord = vec3(inTexCoord, inTexID);}