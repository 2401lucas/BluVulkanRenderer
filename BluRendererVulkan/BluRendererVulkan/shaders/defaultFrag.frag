#version 450layout(location = 0) in vec3 fragColour;layout(location = 1) in vec2 fragTexCoord;layout(binding = 2) uniform sampler2D texSampler;layout(location = 0) out vec4 outColour;layout(set = 0, binding = 1) uniform  GPUSceneData {    vec4 fogColor; // w is for exponent	vec4 fogDistances; //x for min, y for max, zw unused.	vec4 ambientColor;	vec4 sunlightDirection; //w for sun power	vec4 sunlightColor;} sceneData;void main() {	outColour = texture(texSampler, fragTexCoord);}